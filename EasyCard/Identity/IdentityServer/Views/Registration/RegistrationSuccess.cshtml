@using Shared.Helpers
@inject CommonLocalizationService localizer

@{
    ViewData["Title"] = localizer.Get("RegistrationRequestReceived");
}


<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">

            <div class="panel-heading">
                <h3 class="panel-title">@localizer.Get("RegistrationRequestReceived")</h3>
            </div>

            <div class="panel-body">
                @localizer.Get("RegistrationSuccessMessage")
            </div>
        </div>
    </div>

</div>